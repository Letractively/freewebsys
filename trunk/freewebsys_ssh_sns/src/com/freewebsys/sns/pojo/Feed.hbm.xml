<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.freewebsys.sns.pojo.Feed" table="Feed">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="userId" column="userId"/>
		
		<property name="userName" column="userName" length="50"/>
		
		<property name="feedType" column="feedType" length="20"/>
		
		<property name="titleData" column="titleData" length="300"/>
		
		<property name="contentData" column="contentData" length="900"/>
		
		<property name="createTime" column="createTime"/>
		
		<property name="feedTemplateId" column="feedTemplateId"/>
		
	</class>
</hibernate-mapping>
