<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.freewebsys.sns.pojo.Comment" table="Comment">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="appType" column="appType"  length="100"/>
		
		<property name="appId" column="appId" />
		
		<property name="userId" column="userId" />
		
		<property name="comment" column="comment"  length="3000"/>
		
		<property name="createTime" column="createTime" />
		
		<property name="parentId" column="parentId" />
		
	</class>
</hibernate-mapping>
