<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.enable.DynamicMethodInvocation" value="false" />
	<constant name="struts.action.extension" value="action,htm" />
	<constant name="struts.objectFactory" value="spring" />
	<!-- <constant name="struts.i18n.encoding" value="UTF-8" /> -->
	<constant name="struts.devMode" value="false" />
	<constant name="struts.custom.i18n.resources" value="com.freewebsys.sns.web.messages" />
	<constant name="struts.multipart.maxSize" value="1024000000" />

	<include file="struts-default.xml" />
	<package name="struts-web-user" extends="struts-default">
		<interceptors>
			<interceptor name="adminLogin"
				class="com.freewebsys.sns.interceptor.UserLoginInterceptor" />
			<interceptor-stack name="teamwareStack">
				<interceptor-ref name="adminLogin" />
				<interceptor-ref name="i18n" />
				<interceptor-ref name="fileUpload">
					<param name="maximumSize">1024000000</param>
				</interceptor-ref>
				<interceptor-ref name="basicStack" />
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="teamwareStack" />
		
		<!-- SwfUploadAction Begin -->
		<action name="swfUploadFile" class="swfUploadAction"
			method="swfUploadFile">
			<interceptor-ref name="fileUpload">
					<param name="maximumSize">1024000000</param>
				</interceptor-ref>
			<interceptor-ref name="basicStack" />
		</action>
		<!-- SwfUploadAction End -->
		
		<!-- PhotoAction Begin -->
		<action name="addPhoto" class="com.freewebsys.sns.action.PhotoAction"
			method="addPhoto">
			<result name="input">/WEB-INF/sns/photo/PhotoForm.jsp</result>
		</action>
		<action name="deletePhoto" class="com.freewebsys.sns.action.PhotoAction"
			method="deletePhoto">
			<result name="success" type="redirect">/listPhoto.htm</result>
		</action>
		<action name="savePhoto" class="com.freewebsys.sns.action.PhotoAction"
			method="savePhoto">
			<result name="input">/WEB-INF/sns/photo/PhotoForm.jsp</result>
			<result name="success" type="redirect">/listPhoto.htm</result>
		</action>
		<action name="listPhoto" class="com.freewebsys.sns.action.PhotoAction"
			method="listPhoto">
			<result name="input">/WEB-INF/sns/photo/PhotoList.jsp</result>
			<result name="success">/WEB-INF/sns/photo/PhotoList.jsp</result>
		</action>
		<action name="viewPhoto" class="com.freewebsys.sns.action.PhotoAction"
			method="viewPhoto">
			<result name="success">/WEB-INF/sns/photo/PhotoView.jsp</result>
		</action>
