<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.freewebsys.sns.pojo.FeedTemplate" table="FeedTemplate">
		<id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="native" />
		</id>
		
		<property name="feedType" column="feedType" length="20"/>
		
		<property name="title" column="title" length="300"/>
		
		<property name="content" column="content" length="300"/>
		
	</class>
</hibernate-mapping>
